    "url": "git+https://github.com/madfrog54321/KidsAndCozmo.git"
  },
  "keywords": [
    "Cozmo"
  ],
  "author": "Konnor Andrews",
  "bugs": {
    "url": "https://github.com/madfrog54321/KidsAndCozmo/issues"
  },
  "homepage": "https://github.com/madfrog54321/KidsAndCozmo",
  "devDependencies": {
    "electron-builder": "^5.16.0",
    "electron-prebuilt": "^1.2.0"
  },
  "dependencies": {
  },
  "build": {
    "appId": "com.konnor.kidsandcozmo",
    "app-category-type": "public.app-category.productivity"
  }
